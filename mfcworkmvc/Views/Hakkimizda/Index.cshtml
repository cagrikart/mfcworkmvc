    <style>
        .my-container {
            max-width: 1200px;
            margin: auto;
        }

        .image-box img {
            height: 100%;
            width: 100%;
            background-size: cover;
            border-radius: 25px;
        }

        .my-btn {
            background: #fff;
            border: none;
            border-radius: 25px;
            padding: 10px 20px;
            margin-right: 10px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            width: 150px;
            height: 75px;
        }

            .my-btn:hover {
                background: #f0f0f0;
            }

        .stats-section {
            background-color: #000; /* Dark background */
            color: #fff; /* White text */
            padding: 20px 0; /* Some padding */
        }

        .stat {
            text-align: center;
            padding: 20px;
        }

        .card-custom {
            position: relative;
            overflow: hidden;
            border-radius: 10px;
            color: white;
        }

            .card-custom img {
                width: 100%;
                height: auto;
                transition: transform 0.5s ease;
            }

            .card-custom:hover img {
                transform: scale(1.1);
            }

            .card-custom .card-body {
                position: absolute;
                bottom: 0;
                background: rgba(0, 0, 0, 0); /* fully transparent */
                width: 100%;
                padding: 20px;
            }

            .card-custom .card-title {
                margin-bottom: 15px;
                font-size: 20px;
            }

            .card-custom .btn {
                background: transparent;
                border: 1px solid white;
                border-radius: 20px;
                background: white;
                color: black;
                text-align: left;
            }

        .service-header {
            margin-top: 30px;
        }
    </style>
</head>
